
<div class="form-inputs">
    <h1>New Post</h1>
        <%= form_tag posts_path, method: 'post', multipart: true do %> 
        title: <%= text_field_tag 'title' %><br><br>
        content: <%= text_field_tag 'content' %><br><br>
        image: <%= file_field_tag 'image' %><br> 
        <%= submit_tag '글쓰기' %> 
        <% end %> 
        <%= link_to 'Back', posts_path %>
</div>